<?xml version="1.0" encoding="UTF-8"?>
<render-kit>
    #foreach( $renderer in $rendererList )
	<renderer>
        <component-family>${renderer.getComponentFamily()}</component-family>
        <renderer-list>
            #foreach( $rendererInfo in ${renderer.getRendererInfoSet()} )
                <renderer-info>
		            <renderer-name>${rendererInfo.getRendererName()}</renderer-name>
		            <renderer-class>${rendererInfo.getRendererClass()}</renderer-class>
		        </renderer-info>
            #end
        </renderer-list>
    </renderer>
    #end
</render-kit>